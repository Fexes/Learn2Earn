<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RTCGlView">
        <attr name="mainMirror" format="boolean" />
        <attr name="secondMirror" format="boolean" />
        <attr name="mainCoords" format="reference" />
        <attr name="secondCoords" format="reference" />
    </declare-styleable>
    <declare-styleable name="FreeDrawView">
        <attr name="paintColor" format="color"/>
        <attr name="paintWidth" format="dimension"/>
        <attr name="paintAlpha" format="integer"/>
        <attr name="resizeBehaviour" format="enum">
            <enum name="clear" value="0"/>
            <enum name="fitXY" value="1"/>
            <enum name="crop" value="2"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="SeekBarPreference">
        <attr name="min" format="integer" />
        <attr name="stepSize" format="integer" />
    </declare-styleable>
</resources>